	    funcao Doll()
	      {
	      	inteiro makita
		 	logico repetiu = falso
		 	inteiro upar

		 	
	      	faca {

			 escrevaslow("\n*O que voce quer fazer?*", DIALOGO)
			 escrevaslow("\n1) Conversar", VELOCIDADE)
		 	 escrevaslow("\n2) Transfusão de sangue", VELOCIDADE)
		 	 escrevaslow("\n3) Parar de conversar\n", VELOCIDADE)

		 	 leia(makita)

		 	 escolha(makita)
		 	 {
				caso 1:
		 	 	{
		 	 		escrevaslow("\"Ao longo do tempo, incontáveis​caçadores visitaram este sonho.\nOs túmulos aqui permanecem em sua memória. Tudo parece tão distante agora...\"", DIALOGO)
		 	 		pare
		 	 	}
		 	 	caso 2:
		 	 	{
		 	 		escrevaslow("\"Muito bem, deixe que os ecos se tornem sua força. Deixe-me ficar perto. Agora feche os olhos...\"\n", DIALOGO)
		 	 		
					escreva("\n|a____|TRANSFUSÃO|____|\n")
		 	 		escreva("|* -  STATS ATUAIS - *|\n")
		 	 		escreva("|1. VIT - ", classeStats[0],"         *|\n")
		 	 		escreva("|2. VIG - ", classeStats[1],"         *|\n")
		 	 		escreva("|3. FOR - ", classeStats[2],"         *|\n")
		 	 		escreva("|4. SPD - ", classeStats[3],"         *|\n")
		 	 		escreva("|5. BLD - ", classeStats[4],"         *|\n")
		 	 		escreva("|6. ARC - ", classeStats[5],"         *|\n")
		 	 		escreva("|*___________________*|")

		 	 		escrevaslow("\nEscolha para aumentar seus stats: ", VELOCIDADE)
		 	 		leia(upar)

		 	 		se(upar == 1)
		 	 		{
		 	 			escrevaslow("\nVIT: " + classeStats[0] + " --> " + (classeStats[0] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[0] = classeStats[0] + 1
		 	 			limpa()
		 	 		}
		 	 		se(upar == 2)
		 	 		{
		 	 			escrevaslow("\nVIG: " + classeStats[1] + " --> " + (classeStats[1] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[1] = classeStats[1] + 1
		 	 			limpa()
		 	 		}
		 	 		se(upar == 3)
		 	 		{
		 	 			escrevaslow("\"FOR: " + classeStats[2] + " --> " + (classeStats[2] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[2] = classeStats[2] + 1
		 	 			limpa()
		 	 		}
		 	 		se(upar == 4)
		 	 		{
		 	 			escrevaslow("\"SPD: " + classeStats[3] + " --> " + (classeStats[3] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[3] = classeStats[3] + 1
		 	 			limpa()
		 	 		}
		 	 		se(upar == 5)
		 	 		{
		 	 			escrevaslow("\nVIT: " + classeStats[4] + " --> " + (classeStats[4] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[4] = classeStats[4] + 1
		 	 			limpa()
		 	 		}
		 	 		se(upar == 6)
		 	 		{
		 	 			escrevaslow("\nVIT: " + classeStats[5] + " --> " + (classeStats[5] + 1), VELOCIDADE)
		 	 			escrevaslow("\nCusto: 200 ecos de sangue\nMoney atual: " + mrkrabs + "\nAperte [ENTER] para prosseguir:", VELOCIDADE)
		 	 			ENTER()
		 	 			mrkrabs = mrkrabs - 200
		 	 			classeStats[5] = classeStats[5] + 1
		 	 			limpa()
		 	 		}
		 	 		pare
		 	 	}
		 	 	caso 3:
		 	 	{
		 	 		escrevaslow("*Voce sai de perto dela, continuando sua jornada*", DIALOGO)
		 	 	}
		 	 	
		 	 }
	      		
	      	}enquanto(makita != 3)
	      }
	//fim do programa
} 
