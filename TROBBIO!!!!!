		 	ENTER()
			limpa()
		 	gascoigne(4000)
		 	limpa()

		 	escrevaslow("O corpo de Gascoigne cai bruscamente no chão, rosto distorcido pelo flagelo que assombra esta cidade.\nEnrolado em seu pescoço, você encontra uma chave.", DIALOGO)
		 	escrevaslow("\nSubindo as escadas do cemitério, voce destranca o portão usando a chave, após alguns minutos de caminhada, voce se encontra dentro de uma Catedral.\nPotes e bacias com incenso ao seu redor, e no meio da sala, outra lâmpada.", DIALOGO)
		 	escrevaslow("\nMais uma vez, você adentra o Sonho do caçador.\nOlhando ao seu redor, nada aparenta ter mudado, mas voce avista alguem, a mulher de antes aparenta ter acordado.",DIALOGO)
	}
			



	funcao batalha(inteiro numero_turnos, inteiro vida_homiruim){
		
		inteiro dano
		
		vida_inimigo = vida_homiruim
		vidabase = vidabase * classeStats[0]
		dano = (vidabase - vida_inimigo) / 3
		
		bixospeak()
		
		enquanto (numero_turnos > 0 e vida_inimigo > 0){
			
			cadeia frase = "\n|==TURNO: " + numero_turnos
			 
			se(classe == 2020){
			
			escrevaslow(frase, VELOCIDADE)
			escrevaslow("|VIDA: " + vidabase, VELOCIDADE)
			escrevaslow("|VITIMA: " + vida_inimigo, VELOCIDADE)
			
			kombat()
			numero_turnos --

			vidabase = vidabase - dano 
			
			escrevaslow("\nVoce tomou " + dano + " de dano.", DIALOGO)
			escrevaslow("\nsua vida agora eh: " + vidabase, DIALOGO)

			
			se(vida_inimigo <= 0){
				escrevaslow("\n*A fera parou de se mover, Bom trabalho!*", DIALOGO)
				mrkrabs = mrkrabs + 500
				escrevaslow("\n*Voce recebeu " + mrkrabs + " ecos de sangue*",DIALOGO)
				
				vidabase = 250
				ENTER()
				pare
			}
			se(numero_turnos <= 0){
				escrevaslow("\n*A fera escapou, mas nao por muito tempo*", DIALOGO)
				ENTER()
				pare
			}
			se(vidabase <= 0){
				escreva("╦  ╦╔═╗╔═╗╔═╗  ╔╦╗╔═╗╦═╗╦═╗╔═╗╦ ╦")
				escreva("╚╗╔╝║ ║║  ║╣   ║║║║ ║╠╦╝╠╦╝║╣ ║ ║")
				escreva(" ╚╝ ╚═╝╚═╝╚═╝  ╩ ╩╚═╝╩╚═╩╚═╚═╝╚═╝")
				escrevaslow("aperte a para retornar", VELOCIDADE)
				ENTER()
				
				
			}
			}senao{
			
			escrevaslow(frase, VELOCIDADE)
			escrevaslow("|" + nome + ": " + vidabase, VELOCIDADE)
			escrevaslow("|INIMIGO: " + vida_inimigo, VELOCIDADE)
			
			kombat()
			numero_turnos --

			vidabase = vidabase - dano 
			
			escrevaslow("\nVoce tomou " + dano + " de dano.", DIALOGO)
			escrevaslow("\nsua vida agora eh: " + vidabase, DIALOGO)

			
			se(vida_inimigo <= 0){
				escrevaslow("\n*A fera parou de se mover, Bom trabalho!*", DIALOGO)
				mrkrabs = mrkrabs + 500
				escrevaslow("\n*Voce recebeu " + mrkrabs + " ecos de sangue*",DIALOGO)
				
				vidabase = 250
				ENTER()
				pare
			}
			se(numero_turnos <= 0){
				escrevaslow("\n*A fera escapou, mas nao por muito tempo*", DIALOGO)
				ENTER()
				pare
			}
			se(vidabase <= 0){
				escreva("╦  ╦╔═╗╔═╗╔═╗  ╔╦╗╔═╗╦═╗╦═╗╔═╗╦ ╦")
				escreva("╚╗╔╝║ ║║  ║╣   ║║║║ ║╠╦╝╠╦╝║╣ ║ ║")
				escreva(" ╚╝ ╚═╝╚═╝╚═╝  ╩ ╩╚═╝╩╚═╩╚═╚═╝╚═╝")
				escrevaslow("aperte a para retornar", VELOCIDADE)
				ENTER()
				
				
			}

			}
		}

		//verificar se venceu ou nao a batalha depois de n turnos
	}
	

	funcao kombat(){ //funcao combate
		

		inteiro arma
		inteiro kapow = 35
		inteiro gun = 25

		cadeia frasekapow = "\n*Voce deu " + (kapow * classeStats[2]) + " de dano, continue*"
		cadeia frasegun = "\n*Voce atirou e deu " + (gun * classeStats[4]) + " de dano, continue*"
		//se or arma branca: chance de 1 a 3 de causar dano critico
		//se usar arma de fogo: chance de 50% de estunar e ganhar mais um turno
		//caso apos estunar usar arma branca, dar dobro de dano 
		
		escrevaslow("\nEscolha sua açao: ", VELOCIDADE)

		se(classe == 2020){
			para(inteiro i=0; i < 2; i++){
			escreva("\n",i,"° ", equip[i], " ")
		}

		leia(arma)
		
		se(arma == 0){
			sorteio = u.sorteia(1, 3)


			se(sorteio == 1){
				vida_inimigo = vida_inimigo - (kapow * classeStats[2]) * 5
				 escrevaslow("\n*COMBO START: Voce da um soco na boca do cara*", DIALOGO)
				 escrevaslow("\n*voce deu: " + (vida_inimigo - (kapow * classeStats[2]) * 5) + " de dano" , DIALOGO)
				 vida_inimigo = vida_inimigo - (kapow * classeStats[2])
				 escrevaslow("\n*COMBO: Voce mete uma bicuda no inimigo*", DIALOGO)
				 escrevaslow("\n*voce deu: " + (vida_inimigo - (kapow * classeStats[2])) + " de dano" , DIALOGO)
				 vida_inimigo = vida_inimigo - (kapow * classeStats[2])
				 escrevaslow("\n*COMBO: Voce da uma voadora no inimigo*", DIALOGO)
				 escrevaslow("\n*voce deu: " + (vida_inimigo - (kapow * classeStats[2])) + " de dano" , DIALOGO)
				 escrevaslow("\n*COMBO FINISH*", DIALOGO)
			}senao{
				vida_inimigo = vida_inimigo - (kapow * classeStats[2]) *2
				escrevaslow("\nVoce da um chute na cabeça do cara, oof",DIALOGO)
				escrevaslow("\n*voce deu: " + (vida_inimigo - (kapow * classeStats[2]) * 2) + " de dano" , DIALOGO)
			}

			
		
		}senao se (arma == 1) {
			sorteio = u.sorteia(1, 2)
			se(sorteio == 2)
			{
				vida_inimigo = vida_inimigo - (gun * classeStats[4]) * 4
				escrevaslow("\n*Voce joga sua bicicleta no cara, e magicamente encontra outra ao seu lado*", DIALOGO)
				escrevaslow("\n*voce deu: " + (vida_inimigo - (gun * classeStats[4]) * 4) + " de dano" , DIALOGO)
 			}senao{
 				vida_inimigo = vida_inimigo - (gun * classeStats[4]) * 2
 				escrevaslow("\n*Voce deu uma bicicletada no cara, oof*",DIALOGO)
 				escrevaslow("\n*voce deu: " + (vida_inimigo - (gun * classeStats[4]) * 2) + " de dano" , DIALOGO)
 			}
		}
		}senao{
		para(inteiro i=0; i < 2; i++){
			escreva("\n",i,"° ", equip[i], " ")
		}

		leia(arma)
		
		se(arma == 0){
			sorteio = u.sorteia(1, 2)


			se(sorteio == 1){
				vida_inimigo = vida_inimigo - (kapow * classeStats[2]) * 2
				 escrevaslow("\n*Acerto critico, nao falta muito...*", DIALOGO)
				 escrevaslow("\n*voce deu: " + ((kapow * classeStats[2]) * 2) + " de dano" , DIALOGO)
			}senao{
				vida_inimigo = vida_inimigo - (kapow * classeStats[2])
				escrevaslow(frasekapow,DIALOGO)
			}

			
		
		}senao se (arma == 1) {
			sorteio = u.sorteia(1, 2)
			se(sorteio == 2)
			{
				vida_inimigo = vida_inimigo - (gun * classeStats[4]) * 2
				escrevaslow("\n*BOOM! headshot, agora e a sua chance, ataca...*", DIALOGO)
				escrevaslow("\n*voce deu: " + ((gun * classeStats[4]) * 2) + " de dano" , DIALOGO)
 			}senao{
 				vida_inimigo = vida_inimigo - (gun * classeStats[4])
 				escrevaslow(frasegun,DIALOGO)
 			}
		}
		}
	}
		
		funcao bixospeak()//falas que aparecem antes de cada luta(
		{
		
		sorteio = u.sorteia(1, 5)
		 
		 se(sorteio == 1)
		 {
		 	escrevaslow("\nUma fera se aproxima",DIALOGO)
		 }senao se(sorteio == 2){
		 	escrevaslow("\nUm atirador lhe avistou", DIALOGO)
		 }senao se(sorteio == 3){
		 	escrevaslow("\nUm executor te encontrou", DIALOGO)
		 }senao se(sorteio == 4){
		 	escrevaslow("\nAquela coisa antes era humana, agora não é mais", DIALOGO)
		 }senao se(sorteio == 5){
		 	escrevaslow("\nCONTEM DOENÇA, MORTE!", DIALOGO)
		 }

		}
		
		
		funcao huntersdream1()//hub q aparece apos cada boss
		{
		 inteiro path
		 logico repetiu = falso
		 escrevaslow("\nO que voce quer fazer? ",DIALOGO)
		 
            escrevaslow("\n1) Ler as lapides\n", VELOCIDADE)
		  escrevaslow("2) Inspecionar o lugar\n", VELOCIDADE)
		  escrevaslow("3) Entrar na igreja\n", VELOCIDADE)
		  escrevaslow("4) Encostar na lampada denovo\n", VELOCIDADE)
		  
		 
		  faca{

		  	se(repetiu == verdadeiro){
		  		escrevaslow("\nO que voce quer fazer?: ",DIALOGO)
		  	}
		  	
		 	leia(path)
		 	
		 	se(path == 1)
		 	{
