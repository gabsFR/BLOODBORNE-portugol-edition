		 	}

			senao se(path == 3){
				escrevaslow("Voce decide subir as escadas, ao subir, voce encontra as portas de uma pequena igreja. Dentro, voce ve um senhorzinho em uma cadeira de rodas, ele sorri ao ver voce", DIALOGO)

