		funcao gascoigne(inteiro vida_homiruim){
			
			inteiro dano
			vida_inimigo = vida_homiruim
			vidabase = vidabase * classeStats[0]
			dano = (vidabase - vida_inimigo) / 3
	
			
			escreva("                                           ═«═╣███▓▀▀Ü\n")
			escreva("                                             -▓█▓╫╬▄▓N\n")
			escreva("                                              ,▓▓▓█▓▓▓▓φ,\n")
			escreva("                                             ╔████▓███████▄\n")
			escreva("                                            .████████████▓█▓\n")
			escreva("                                            ▓████████▓█████▓▓≥\n")
			escreva("                                           ╔▓▓████████▌█████▓▓▄\n")
			escreva("                                           ▓█▓████████▓█████▓▓█▄\n")
			escreva("                                          ╓█▓████████▓███████▓▓▌\n")
			escreva("                                         .██╩████████▓▓████╫█▓▓M\n")
			escreva("                                         ╝▀▀╔████████▓▓███▀▓█▓█⌐\n")
			escreva("                                            ▓███████▓▓▓█▓█ ▓███\n")
			escreva("                                           ║████████▓▓▓███,▓▓▓▌\n")
			escreva("                                           ▓████████▓▓▓▓██▓╫╫╫Ñ\n")
			escreva("                                           ▓████▓████▓▓▓█▓▓▌╫╫`\n")
			escreva("                                       .«╗▓██▓▓▓███████▓▌▓█▀`\n")
			escreva("                       ╓╦ΦΦ╗▄µ╓,,╓▄▄╗▓▓╫Å╨`▐█▓▓▓███████▓██▌\n")
			escreva("                 ^╜▀▀▀╝▓▓▓▓▓▓▓▓▓▓▓▀▀╨`     j█▓▓▓██████▌╫██▓\n")
			escreva("                      ╔╫▓▓▓▓▓▌    .         █▓███████▌▓█▓██\n")
			escreva("                    ,╓╣▓▓▓▓███▓▓▓▓▌         ▓████████╫▓▓▓██\n")
			escreva("                 ╟╫▓▓▓▓▓▓▓▓██████▀          ╣███████╫▓█▓▓██⌐\n")
			escreva("                  ╙╣▓▓▓████████▀            ║█████▓▌▓▓▓█▓██M\n")
			escreva("                    `╙▀▀▓▓▓▀▀^              ║█████▓╫██▓████▌\n")
			escreva("                                            ╟██████▀▀███▓██▌\n")
			escreva("                                            ║╙`████H ▓▓█▓███\n")
			escreva("                                            \"  ▓███▌ j▓█████▄\n")
			escreva("                                               ▓███▌ ▐██████▓\n")
			escreva("                                              ╔▓▓███ ▐▓██▓███M\n")
			escreva("                                          :╦╬╫╫▓▀▀▀╨  ▓▓▓▓█▀`\n")
			escreva("                                                      ╣▌╫▓M\n")
	
			escreva("\nPADRE GASCOIGNE AVANÇA")

			logico ja_se_transformou = falso
			
			enquanto (vida_inimigo > 0){
				
			escrevaslow("|" + nome + ": " + vidabase, VELOCIDADE)
			escrevaslow("|PADRE GASCOIGNE: " + vida_inimigo, VELOCIDADE)
			
			kombat()
			
			vidabase = vidabase - dano 
			
			escrevaslow("\nVoce tomou " + dano + " de dano.", DIALOGO)
			escrevaslow("\nsua vida agora eh: " + vidabase, DIALOGO)

			
				
			
			
			
				se(vida_inimigo <= 2000 e ja_se_transformou == falso ){
					
					vida_inimigo = vida_inimigo + 3000
					vidabase = vidabase + 2700
					dano = (vidabase - vida_inimigo) / 2
	
					escrevaslow("\n\nPadre gascoigne começa a gritar, lentamente, voce ve seu corpo, mudando.\n Ele fica maior, garras surgindo em suas maos, pelos cubrindo seu corpo inteiro.\n", DIALOGO)
					ENTER()
					limpa()
	
					   escreva("              .,µµ^`,\n")
				        escreva("             ╦╦╦Ü╫` n░\n")
				        escreva("           ╔╦╬╩▓M╫░░╦▄▄,\n")
